{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with GDB",
            "type":"cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/c8oa",
            "args": [
                "${workspaceFolder}/roms/4-flags.ch8"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for GDB",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build with Make"
        }
    ]
}